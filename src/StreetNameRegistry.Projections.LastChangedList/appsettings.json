{
  "ConnectionStrings": {
    "Events": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.StreetNameRegistry;Trusted_Connection=True;",
    "LastChangedList": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.StreetNameRegistry;Trusted_Connection=True;"
  },

  "DataDog": {
    "ServiceName": "streetname-registry-lastchangedlist-projections"
  },

  "CatchUpThreshold" : 1000,
  "CatchUpPageSize": 1000,

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "StreetNameRegistry": "Debug",
      "Aiv": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "StreetNameRegistry": "Verbose",
        "Aiv": "Verbose",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },

    "Properties": {
      "Application": "StreetNameRegistry - LastChangedList Projections"
    }
  }
}
